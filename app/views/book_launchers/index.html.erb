<h1>Listing book_launchers</h1>

<table>
  <thead>
    <tr>
      <th>Book</th>
      <th>Launcher</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @book_launchers.each do |book_launcher| %>
      <tr>
        <td><%= book_launcher.book_id %></td>
        <td><%= book_launcher.launcher_id %></td>
        <td><%= link_to 'Show', book_launcher %></td>
        <td><%= link_to 'Edit', edit_book_launcher_path(book_launcher) %></td>
        <td><%= link_to 'Destroy', book_launcher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book launcher', new_book_launcher_path %>
